int foo(int a[2][3]) {
    int i = 0, j = 0;
    while (i < 2) {
        j = 0;
        while (j < 3) {
            write(a[i][j]);
            j = j + 1;
        }
        i = i + 1;
    }
    return 0;
}

int main() {
    int b[2][2][3];
    int k = 0, m = 0, n = 0;
    while (k < 2) {
        m = 0;
        while (m < 2) {
            n = 0;
            while (n < 3) {
                b[k][m][n] = k + m + n;
                n = n + 1;
            }
            m = m + 1;
        }
        k = k + 1;
    }
    foo(b[0]);
    foo(b[1]);
    return 0;
}
